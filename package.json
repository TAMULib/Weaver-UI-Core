{
  "name": "weaver-ui-core",
  "private": false,
  "version": "2.0.0-RC1",
  "description": "Weaver AngularJs Core Module",
  "repository": " https://github.com/TAMULib/Weaver-UI-Core.git",
  "license": "MIT",
  "bin": {
    "wvr": "cli/weaverCli.js"
  },
  "scripts": {
    "prestart": "npm install -g grunt-cli",
    "pretest": "npm install",
    "postinstall": "grunt",
    "test": "karma start karma.conf.js",
    "test-single-run": "karma start karma.conf.js --single-run",
    "test-ci": "karma start karma.conf.js --no-auto-watch --single-run --browsers=Chrome",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor e2e-tests/protractor.conf.js"
  },
  "dependencies": {
    "angular": "1.6.5",
    "angular-loader": "1.6.5",
    "angular-messages": "1.6.5",
    "angular-mocks": "1.6.5",
    "angular-route": "1.6.5",
    "angular-sanitize": "1.6.5",
    "angular-ui-bootstrap": "2.5.0",
    "bootstrap": "3.3.7",
    "bootstrap-sass": "3.3.7",
    "express": "4.15.4",
    "forever": "0.15.3",
    "forever-monitor": "1.7.1",
    "html5-boilerplate": "6.0.0",
    "jquery": "3.2.1",
    "ng-file-upload": "12.2.13",
    "sockjs-client": "1.1.4",
    "stompjs": "2.3.3"
  },
  "devDependencies": {
    "grunt": "1.0.1",
    "grunt-cli": "1.2.0",
    "grunt-contrib-jshint": "1.1.0",
    "grunt-karma-coveralls": "2.5.4",
    "grunt-ngdocs": "0.2.10",
    "jshint-stylish": "2.2.1",
    "karma": "1.7.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage": "1.1.1",
    "karma-firefox-launcher": "1.0",
    "karma-jasmine": "1.1.0",
    "karma-junit-reporter": "1.2.0",
    "karma-ng-html2js-preprocessor": "1.0.0",
    "protractor": "5.1.2"
  }
}
