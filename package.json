{
  "name": "@wvr/core",
  "version": "2.2.2-rc.7",
  "description": "Weaver AngularJs Core Module",
  "repository": " https://github.com/TAMULib/Weaver-UI-Core.git",
  "license": "MIT",
  "private": false,
  "bin": {
    "wvr": "cli/weaverCli.js"
  },
  "scripts": {
    "pretest": "npm install",
    "test": "karma start karma.conf.js",
    "test-single-run": "karma start karma.conf.js --single-run",
    "test-ci": "karma start karma.conf.js --no-auto-watch --single-run --browsers=ChromeHeadless",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor e2e-tests/protractor.conf.js",
    "start:registry": "verdaccio --listen localhost:4873 --config ./verdaccio-config.yaml",
    "publish:local": "npm publish",
    "prepublish:local": "shx cp .npmrc-localhost .npmrc",
    "postpublish:local": "shx rm -f .npmrc"
  },
  "dependencies": {
    "angular": "1.8.2",
    "angular-loader": "1.8.2",
    "angular-messages": "1.8.2",
    "angular-mocks": "1.8.2",
    "angular-route": "1.8.2",
    "angular-sanitize": "1.8.2",
    "angular-ui-bootstrap": "2.5.6",
    "bootstrap": "5.1.3",
    "bootstrap-sass": "3.4.3",
    "express": "4.17.3",
    "forever": "4.0.3",
    "forever-monitor": "3.0.3",
    "html5-boilerplate": "8.0.0",
    "https": "^1.0.0",
    "jquery": "3.6.0",
    "ng-file-upload": "12.2.13",
    "sockjs-client": "1.6.0",
    "stompjs": "2.3.3"
  },
  "devDependencies": {
    "grunt": "1.4.1",
    "grunt-cli": "^1.4.3",
    "grunt-contrib-jshint": "^3.2.0",
    "grunt-karma-coveralls": "^2.5.4",
    "grunt-ngdocs": "^0.2.11",
    "jshint-stylish": "2.2.1",
    "karma": "6.3.17",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage": "2.2.0",
    "karma-firefox-launcher": "2.1",
    "karma-jasmine": "4.0.2",
    "karma-junit-reporter": "2.0.1",
    "karma-ng-html2js-preprocessor": "1.0.0",
    "protractor": "7.0.0",
    "shx": "^0.3.4",
    "verdaccio": "^5.13.1",
    "verdaccio-memory": "^10.3.0"
  }
}
